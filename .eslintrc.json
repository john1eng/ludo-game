{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard", "plugin:jest/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": false
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": ["react", "jest"],
  "rules": {
      "camelcase": "off",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off",
      "quotes": ["off"],
      "semi": [2, "always"]
  }
}
